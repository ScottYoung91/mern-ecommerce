{
	"name": "mern-ecommerce",
	"version": "1.0.0",
	"type": "module",
	"description": "MERN shopping cart app",
	"main": "server.js",
	"scripts": {
		"start": "node backend/server.js",
		"server": "nodemon backend/server.js",
